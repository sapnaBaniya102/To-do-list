#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/sapana/To-do-list/node_modules/@nuxt/cli/bin/node_modules:/Users/sapana/To-do-list/node_modules/@nuxt/cli/node_modules:/Users/sapana/To-do-list/node_modules/@nuxt/node_modules:/Users/sapana/To-do-list/node_modules:/Users/sapana/node_modules:/Users/node_modules:/node_modules:/Users/sapana/To-do-list/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/sapana/To-do-list/node_modules/@nuxt/cli/bin/node_modules:/Users/sapana/To-do-list/node_modules/@nuxt/cli/node_modules:/Users/sapana/To-do-list/node_modules/@nuxt/node_modules:/Users/sapana/To-do-list/node_modules:/Users/sapana/node_modules:/Users/node_modules:/node_modules:/Users/sapana/To-do-list/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@nuxt/cli/bin/nuxt-cli.js" "$@"
else
  exec node  "$basedir/../@nuxt/cli/bin/nuxt-cli.js" "$@"
fi
