#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/sapana/To-do-list/node_modules/.pnpm/nuxt@2.17.1_consola@3.2.3_vue@2.7.14/node_modules/nuxt/bin/node_modules:/Users/sapana/To-do-list/node_modules/.pnpm/nuxt@2.17.1_consola@3.2.3_vue@2.7.14/node_modules/nuxt/node_modules:/Users/sapana/To-do-list/node_modules/.pnpm/nuxt@2.17.1_consola@3.2.3_vue@2.7.14/node_modules:/Users/sapana/To-do-list/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/sapana/To-do-list/node_modules/.pnpm/nuxt@2.17.1_consola@3.2.3_vue@2.7.14/node_modules/nuxt/bin/node_modules:/Users/sapana/To-do-list/node_modules/.pnpm/nuxt@2.17.1_consola@3.2.3_vue@2.7.14/node_modules/nuxt/node_modules:/Users/sapana/To-do-list/node_modules/.pnpm/nuxt@2.17.1_consola@3.2.3_vue@2.7.14/node_modules:/Users/sapana/To-do-list/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../nuxt/bin/nuxt.js" "$@"
else
  exec node  "$basedir/../nuxt/bin/nuxt.js" "$@"
fi
